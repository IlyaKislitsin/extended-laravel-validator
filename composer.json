{
    "name": "avto-dev/extended-laravel-validator",
    "description": "Extended validation rules for Laravel applications",
    "keywords": [
        "laravel",
        "validator",
        "extended",
        "identity"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Avto Develop",
            "homepage": "https://github.com/avto-dev"
        }
    ],
    "require": {
        "php": "^5.6 || >=7.0 <=7.3",
        "laravel/framework": ">=5.4.0 <5.7.0"
    },
    "require-dev": {
        "laravel/laravel": ">=5.4.0 <5.7.0",
        "mockery/mockery": "0.9.* || ~1.0",
        "phpunit/phpunit": "~5.7.10 || ^6.4 || ~7.0"
    },
    "autoload": {
        "psr-4": {
            "AvtoDev\\ExtendedLaravelValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvtoDev\\ExtendedLaravelValidator\\Tests\\": "tests"
        }
    },
    "support": {
        "issues": "https://github.com/avto-dev/extended-laravel-validator/issues",
        "source": "https://github.com/avto-dev/extended-laravel-validator"
    },
    "scripts": {
        "test": "@php ./vendor/bin/phpunit --no-coverage",
        "test-cover": "@php ./vendor/bin/phpunit --coverage-html='./coverage/html'"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AvtoDev\\ExtendedLaravelValidator\\ExtendedValidatorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
